{
  "name": "kellyapps-monorepo",
  "private": true,
  "version": "0.2.0",
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm --filter @kellyapps/fei dev",
    "dev:all": "pnpm -r --parallel run dev",
    "lint": "biome check .",
    "format": "biome format .",
    "test": "pnpm -r run test"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.0",
    "tsx": "^4.16.2"
  }
}
